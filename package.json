  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "axios": "^0.18.0",
    "events": "^3.0.0",
    "kofo-utils": "^1.0.1",
    "lodash": "^4.17.11",
    "mqtt": "^2.18.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
